    "husky": "^7.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^13.0.1",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
